alias fcode R1;
alias currPID R2;
if(fcode==8)then        //Acquire Terminal
    if([TERMINAL_STATUS_TABLE]==1)then
        [PROCESS_TABLE + [SYSTEM_STATUS_TABLE+1] * 16 + 4] = WAIT_TERMINAL;
        multipush(R1,R2);
        call SCHEDULER;
        multipop(R1,R2);
    endif;
    [TERMINAL_STATUS_TABLE]=1;
    [TERMINAL_STATUS_TABLE+1]=currPID;
else if(fcode==9)then   //Release Terminal
    if(currPID!=[TERMINAL_STATUS_TABLE+1])then
        R0=-1;
        return;
    endif;
    [TERMINAL_STATUS_TABLE]=0;
    alias pte R3;
    pte=PROCESS_TABLE;
    while(pte<(PROCESS_TABLE +16*15+1))do
        if([pte+4]==WAIT_TERMINAL && [pte+1]!=-1)then
            [pte+4]=READY;
        endif;
        pte=pte+16;
    endwhile;
    R0=0;
endif;
endif;
breakpoint;
return;